  <meta name="viewport" content="width=device-width,initial-scale=1">
    <title><%=name %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
  <div class="excer-all">
  <img src="http://localhost:3000/images/userimage.png" alt="user image for user homepage" class="excersise-image">
  <h1 class="excersise-name-all"><%=name %></h1>
    <!--set up divs to contain images of workout along with description and name of workout 
    insert for loop to go through workout obj--> 
    <ul>
    <% for(var i = 0; i < bodyParts.length; i++){ %>
      <li><%=bodyParts[i] %></li>
    <%} %>
    </ul>
    <p class="equipment-all"><%=equipment %></p>
    </ul>
